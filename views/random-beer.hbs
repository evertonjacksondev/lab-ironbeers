<div class="beerRandom">
  {{#each beers}}
    {{! image, name, description and tagline. }}
    {{> beerpartial this}}
    <h4>Food Pairing</h4>
    <ul>
        {{#each this.food_pairing}}
            <li>{{this}}</li>
        {{/each}}
    </ul>
    <h4>Brewers Tips</h4>
    <p>{{this.brewers_tips}}</p>
  {{/each}}
</div>